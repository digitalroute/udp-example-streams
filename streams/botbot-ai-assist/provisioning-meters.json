      "schemaVersion": 1,
      "name": "Meter Type FIlter",
      "params": {
        "transform": "await push({\n    userIds: ['Cloud AI', 'Machine Emotions'],\n    meterTypeName: 'L1_Claude3_Sonnet_Requests',\n});\nawait push({\n    userIds: ['Play-to-Earn', 'Android Sheep'],\n    meterTypeName: 'L1_Gemini_Pro_Requests',\n});\nawait push({\n    userIds: ['FinTech Lords', 'Metaverse Mages'],\n    meterTypeName: 'L1_Llama3_Requests',\n});\nawait push({\n    userIds: ['Cloud AI', 'Machine Emotions'],\n    meterTypeName: 'L2_Claude3_Opus_Requests',\n});\nawait push({\n    userIds: ['Play-to-Earn', 'Metaverse Mages'],\n    meterTypeName: 'L2_Gemini_Flash_Requests',\n});\nawait push({\n    userIds: ['FinTech Lords', 'Android Sheep'],\n    meterTypeName: 'L2_GPT4o_Requests',\n});\n"
      },
      "x": 550,
      "y": 150
    },
